<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="HubSpot Regression parallel  test suite " thread-count="2"
	parallel="tests">
  <test name="Regression Functional Sanity Test ---> chrome"  verbose="2" >
     <parameter name="browser" value="chrome" />
    
    <classes>
      
      <class name="com.qa.hubspot.test.LoginPageTest"/>
    </classes>
  </test>
  
  
   <test  name="Regression Functional Sanity Test ---> firefox" verbose="2" >
        <parameter name="browser" value="firefox" />
         
     <classes>
      
      <class name="com.qa.hubspot.test.HomePageTest"/>
    </classes>
  </test>
  
</suite> 